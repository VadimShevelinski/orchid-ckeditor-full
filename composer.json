{
  "name": "vadimshevelinski/orchid-ckeditor-full",
  "description": "CKEditor5 editor for Laravel Orchid",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vadim Shevelinski",
      "email": "shevelinski@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "orchid/platform": "^14.0"
  },
  "autoload": {
    "psr-4": {
      "VadimShevelinski\\OrchidCKEditor\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VadimShevelinski\\OrchidCKEditor\\CKEditorServiceProvider"
      ]
    }
  },
  "support": {
    "source": "https://github.com/vadimshevelinski/orchid-ckeditor-full",
    "issues": "https://github.com/vadimshevelinski/orchid-ckeditor-full/issues"
  }
}
